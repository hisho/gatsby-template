{
  "name": "gatsby-template",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "preinstall": "npx check-engines && npx only-allow npm",
    "start": "NODE_ENV=development run-p start:gulp gatsby:start watch:type-check watch:typed-css",
    "build": "NODE_ENV=production run-s build:type-check build:typed-css gatsby:clean build:gulp gatsby:build",
    "serve": "NODE_ENV=production run-s build gatsby:serve",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore . --fix",
    "gulp:image": "gulp image",
    "gulp:watch": "gulp watch",
    "start:gulp": "run-s gulp:image gulp:watch",
    "build:gulp": "run-s gulp:image",
    "gatsby:start": "gatsby develop -o -H 0.0.0.0",
    "gatsby:build": "gatsby build",
    "gatsby:serve": "gatsby serve -o -H 0.0.0.0",
    "gatsby:clean": "gatsby clean",
    "build:type-check": "tsc --noEmit",
    "watch:type-check": "tsc --noEmit -w",
    "build:typed-css": "tcm -c -p \"src/**/*.module.css\" .",
    "watch:typed-css": "tcm -c -p \"src/**/*.module.css\" . -w",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --ignore-path .gitignore . --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "body-scroll-lock": "^3.1.5",
    "focus-trap-react": "^8.4.2",
    "gatsby": "^3.3.0",
    "gatsby-plugin-google-gtag": "^3.3.0",
    "gatsby-plugin-image": "^1.3.0",
    "gatsby-plugin-no-sourcemaps": "^3.3.0",
    "gatsby-plugin-postcss": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-remove-console": "^0.0.2",
    "gatsby-plugin-remove-generator": "^1.1.0",
    "gatsby-plugin-sharp": "^3.3.0",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-div-100vh": "^0.6.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.1.1",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/eslint": "^7.2.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/fs-extra": "^9.0.11",
    "@types/glob": "^7.1.3",
    "@types/gulp": "^4.0.8",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.41",
    "@types/postcss-import": "^12.0.0",
    "@types/postcss-nested": "^4.2.3",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "fs-extra": "^9.1.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-import": "^14.0.1",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typed-css-modules": "^0.6.7",
    "typescript": "^4.2.4"
  }
}
