{
  "name": "gatsby-template",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "preinstall": "npx check-engines && npx only-allow npm",
    "start": "NODE_ENV=development run-p gatsby:start watch:type-check watch:typed-css",
    "build": "NODE_ENV=production run-s build:type-check build:typed-css gatsby:clean gatsby:build",
    "serve": "NODE_ENV=production run-s build gatsby:serve",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore . --fix",
    "gatsby:start": "gatsby develop -o -H 0.0.0.0",
    "gatsby:build": "gatsby build",
    "gatsby:serve": "gatsby serve -o -H 0.0.0.0",
    "gatsby:clean": "gatsby clean",
    "build:type-check": "tsc --noEmit",
    "watch:type-check": "tsc --noEmit -w",
    "build:typed-css": "tcm -c -p \"src/**/*.module.css\" .",
    "watch:typed-css": "tcm -c -p \"src/**/*.module.css\" . -w"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "gatsby": "^3.0.1",
    "gatsby-plugin-google-analytics": "^3.0.0",
    "gatsby-plugin-image": "^1.0.1",
    "gatsby-plugin-postcss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.1",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.0.3",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@tailwindcss/jit": "^0.1.3",
    "@types/node": "^14.14.33",
    "@types/postcss-import": "^12.0.0",
    "@types/postcss-nested": "^4.2.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.2.1",
    "typed-css-modules": "^0.6.5",
    "typescript": "^4.2.3"
  }
}
