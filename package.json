{
  "name": "gatsby-template",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "preinstall": "npx check-engines && npx only-allow npm",
    "start": "run-p gatsby:start watch:type-check",
    "build": "run-s build:type-check gatsby:clean gatsby:build",
    "serve": "run-s build gatsby:serve",
    "gatsby:start": "gatsby develop -o -H 0.0.0.0",
    "gatsby:build": "gatsby build",
    "gatsby:serve": "gatsby serve -o -H 0.0.0.0",
    "gatsby:clean": "gatsby clean",
    "build:type-check": "tsc --noEmit",
    "watch:type-check": "tsc --noEmit -w"
  },
  "dependencies": {
    "gatsby": "^3.0.1",
    "gatsby-plugin-google-analytics": "^3.0.0",
    "gatsby-plugin-image": "^1.0.1",
    "gatsby-plugin-postcss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.1",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.0.3",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "autoprefixer": "^10.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.5",
    "typescript": "^4.2.3"
  }
}
