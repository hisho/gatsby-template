{
  "name": "gatsby-template",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=14.8.0"
  },
  "scripts": {
    "preinstall": "npx check-engines && npx only-allow npm",
    "start": "run-p typed-scss-watch type-watch gatsby-develop",
    "serve": "run-s build gatsby-serve",
    "build": "run-s gatsby-clean type-check typed-scss gatsby-build",
    "gatsby-develop": "gatsby develop -o -H 0.0.0.0",
    "gatsby-serve": "gatsby serve -o -H 0.0.0.0",
    "gatsby-build": "gatsby build",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json,md}\"",
    "env": "touch .env.production .env.development",
    "gatsby-clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "type-watch": "tsc --noEmit -w",
    "typed-scss": "tsm \"src/**/*.module.scss\" --implementation sass",
    "typed-scss-watch": "tsm \"src/**/*.module.scss\" --implementation sass -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.11.0",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-canonical-urls": "^2.3.13",
    "gatsby-plugin-emotion": "^4.3.15",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "^3.3.2",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-remove-generator": "^1.0.7",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-sitemap": "^2.5.1",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-transformer-json": "^2.4.15",
    "gatsby-transformer-sharp": "^2.5.21",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^1.9.6",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "autoprefixer": "^9.8.6",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "prettier": "2.1.2",
    "sass": "^1.29.0",
    "twin.macro": "^1.12.0",
    "typed-scss-modules": "^3.3.0",
    "typescript": "^4.0.5",
    "typescript-plugin-tw-template": "^2.1.0",
    "utility-types": "^3.10.0"
  }
}
